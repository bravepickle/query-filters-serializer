{
    "name": "bravepickle/query-filters-serializer",
    "description": "Serializer of query filters from arrays or strings. Provides constraints and parsing logic for preconfigured filters",
    "keywords": [
        "php",
        "api",
        "query",
        "search",
        "filters",
        "serializer",
        "url filter",
        "api filter",
        "http filter",
        "form filter",
        "parse filters",
        "request query filter"
    ],
    "require": {
        "php": ">=5.5",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">4"
    },
    "license": "GNU LGPL",
    "authors": [
        {
            "name": "Victor K",
            "email": "2bravepickle@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "QueryFilterSerializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QueryFilterSerializerTest\\": "src/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
